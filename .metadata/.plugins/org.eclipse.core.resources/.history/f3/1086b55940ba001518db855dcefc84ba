import requests

r = requests.get('http://localhost:8000/sas/users/?format=json')
if r.status_code == 200:
	print r.text
	val = r.json()
	for rec in val:
		for k in rec:
			print k,": ", rec[k]
 

r = requests.get('http://localhost:8000/sas/users/1/?format=json')

print r.status_code
if r.status_code == 200:
	val = r.json()
	for k in val:
		print k,": ",val[k]